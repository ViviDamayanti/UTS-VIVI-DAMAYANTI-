    private void tambahActionPerformed(java.awt.event.ActionEvent evt) {                                       
bil1 = Double.parseDouble(angka);
tampilteks.setText("+");
angka = "";
pilih=1;        // TODO add your handling code here:
    }                                      

    private void kaliActionPerformed(java.awt.event.ActionEvent evt) {                                     
