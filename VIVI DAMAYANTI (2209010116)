bil2 = 0.0;
jumlah = 0.0;
angka = "";
// TODO add your handling code here:
    }                                     

    private void kurangActionPerformed(java.awt.event.ActionEvent evt) {                                       
        bil1 = Double.parseDouble(angka);
    tampilteks.setText("-");
    angka = "";
    pilih=2;
// TODO add your handling code here:
    }                                      

